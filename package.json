    "moment": "^2.29.1",
    "prettier": "^2.1.2",
    "prop-types": "^15.6.0",
    "react": "16.13.1",
    "react-autosize-textarea": "^4.0.0",
    "react-dom": "16.13.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
