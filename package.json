    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "devStart": "node %NODE_DEBUG_OPTION% ./node_modules/react-scripts/scripts/start.js",
    "build_dev": "cross-env REACT_APP_STAGE=dev react-scripts build",
    "build_dev2": "cross-env REACT_APP_STAGE=dev2 react-scripts build --profile",
    "build_dev3": "cross-env REACT_APP_STAGE=dev3 react-scripts build",
    "build_prod": "cross-env REACT_APP_STAGE=prod react-scripts build",
    "build_v2prod": "cross-env REACT_APP_STAGE=v2prod react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "prettier": "prettier --write \"src/**/*.{css,js}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "browserslist": {
    "development": [
      "ie 11",
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.3",
    "@date-io/date-fns": "1.x",
    "@hookform/resolvers": "^0.1.0",
    "@material-ui/core": "^4.9.12",
    "@material-ui/pickers": "^3.2.10",
    "@sentry/react": "^5.26.0",
    "@sentry/tracing": "^5.26.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "apollo-link": "^1.2.14",
    "aws-amplify": "^3.3.4",
    "aws-appsync": "^4.0.0",
    "aws-appsync-auth-link": "^3.0.2",
    "aws-appsync-subscription-link": "^3.0.3",
    "d3": "^5.15.0",
    "date-fns": "^2.12.0",
    "jss": "^10.0.0",
    "query-string": "^6.13.5",
    "rc-slider": "^9.5.3",
    "react-adopt": "^0.6.0",
    "react-app-polyfill": "^1.0.6",
    "react-beautiful-dnd": "^13.0.0",
    "react-hook-form": "^6.9.2",
    "source-map-explorer": "^2.5.0",
    "styled-components": "^5.1.1",
    "typescript": "^4.0.3",
    "yup": "^0.29.3"
  }
}
